{
    "plugins": ["prettier", "compat"],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:compat/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    }
}
