APP_NAME="DUS-i Portal Backend"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_PORT=8001

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_APPLICATION_DATABASE=portal_backend
DB_APPLICATION_HOST=portal-backend-pgsql
DB_APPLICATION_PORT=5432
DB_APPLICATION_USERNAME=postgres
DB_APPLICATION_PASSWORD=password
DB_APPLICATION_SSLMODE=prefer
DB_APPLICATION_SSLROOTCERT=null
DB_APPLICATION_SSLCERT=null
DB_APPLICATION_SSLKEY=null

DB_FORM_DATABASE=form_admin_web
DB_FORM_HOST=form-admin-web-pgsql
DB_FORM_PORT=5432
DB_FORM_USERNAME=postgres
DB_FORM_PASSWORD=password
DB_FORM_SSLMODE=prefer
DB_FORM_SSLROOTCERT=null
DB_FORM_SSLCERT=null
DB_FORM_SSLKEY=null

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=rabbitmq
SESSION_DRIVER=file
