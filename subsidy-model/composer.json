{
    "name": "minvws/dusi-subsidy-model",
    "description": "Models and repositories for the subsidy model",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thomas-samoht",
            "email": "54805624+thomas-samoht@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "phpstan/phpstan": "^1.10",
        "phpmd/phpmd": "^2.13",
        "fakerphp/faker": "^1.23",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.13",
        "orchestra/testbench": "^8.0",
        "vimeo/psalm": "^5.14",
        "nunomaduro/larastan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MinVWS\\DUSi\\Shared\\Subsidy\\": "src/",
            "MinVWS\\DUSi\\Shared\\Subsidy\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinVWS\\DUSi\\Shared\\Subsidy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MinVWS\\DUSi\\Shared\\Subsidy\\SubsidyServiceProvider"
            ]
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon.dist",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
