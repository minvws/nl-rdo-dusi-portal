{
    "type": "FormGroupControl",
    "options": {
        "section": true
    },
    "elements": [
        {
            "type": "FormGroupControl",
            "label": "Beoordeling",
            "options": {
                "section": true,
                "headingLevel": "2"
            },
            "elements": [
                {
                    "type": "CustomControl",
                    "scope": "#/properties/assessmentDelayChecklist",
                    "options": {
                        "readonly": true,
                        "format": "checkbox-group"
                    }
                }
            ]
        },
        {
            "type": "CustomControl",
            "scope": "#/properties/assessment",
            "options": {
                "readonly": true,
                "format": "radio"
            }
        },
        {
            "type": "FormResultsTable",
            "options": {
                "fields": {
                    "Bewijs": "{proof}",
                    "Interne notitie": "{internalNote}"
                },
                "showEmptyFields": true
            }
        }
    ]
}
