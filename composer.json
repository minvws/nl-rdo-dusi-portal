{
    "name": "minvws/subsidy-model",
    "description": "Models and repositories for form-admin-web",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thomas-samoht",
            "email": "54805624+thomas-samoht@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "phpstan/phpstan": "^1.10",
        "fakerphp/faker": "^1.23",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.13"
    },
    "autoload": {
        "psr-4": {
            "MinVWS\\SubsidyModel\\": "src/MinVWS/SubsidyModel/",
            "MinVWS\\SubsidyModel\\Database\\Factories\\": "src/MinVWS/SubsidyModel/database/factories/",
            "MinVWS\\SubsidyModel\\Database\\Seeders\\": "src/MinVWS/SubsidyModel/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinVWS\\Tests\\SubsidyModel\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MinVWS\\SubsidyModel\\SubsidyModelServiceProvider"
            ]
        }
    },

    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon.dist",
        "phpcs": "vendor/bin/phpcs"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
