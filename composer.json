{
    "name": "minvws/nl-rdo-dusi-assessment-web",
    "type": "project",
    "description": "",
    "keywords": [],
    "license": "EUPL-1.2",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.7",
        "laravel/fortify": "^1.17.4",
        "laravel/framework": "^10.15",
        "predis/predis": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8.1",
        "barryvdh/laravel-ide-helper": "^2.13.0",
        "fakerphp/faker": "^1.23.0",
        "jorijn/laravel-security-checker": "^2.4.0",
        "laravel/pint": "^1.10.5",
        "laravel/sail": "^1.23.1",
        "laravel/tinker": "^2.8.1",
        "mockery/mockery": "^1.6.4",
        "nunomaduro/collision": "^7.1.0",
        "nunomaduro/larastan": "^2.6.3",
        "phpmd/phpmd": "^2.13.0",
        "phpunit/phpunit": "^9.6.10",
        "psalm/plugin-laravel": "^2.8.0",
        "slevomat/coding-standard": "^8.13.4",
        "spatie/laravel-ignition": "^2.2.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "vimeo/psalm": "^5.13.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
