{
    "name": "minvws/dusi-serialisation-model",
    "description": "Models and classes for the rabbitmq serialisation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerbrand Bosch",
            "email": "gerbrandb@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.16",
        "minvws/codable": "dev-feature/codes-trait as 1.0.x-dev"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2",
        "slevomat/coding-standard": "^8.13",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.14"
    },
    "autoload": {
        "psr-4": {
            "MinVWS\\DUSi\\Shared\\Serialisation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinVWS\\DUSi\\Shared\\Serialisation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon.dist",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf",
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:minvws/nl-rdo-php-codable.git"
        }
    ]
}
